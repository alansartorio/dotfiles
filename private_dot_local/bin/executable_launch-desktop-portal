/usr/lib/xdg-desktop-portal-hyprland > /tmp/xdg-desktop-portal-hyprland.log &
#/usr/lib/xdg-desktop-portal-gtk &
sleep 1
#sleep 1
/usr/lib/xdg-desktop-portal > /tmp/xdg-desktop-portal.log &

